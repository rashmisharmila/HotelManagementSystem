         <yAxis>
            <NumberAxis fx:id="income2" label="Income(Rs)" side="LEFT" tickLabelFill="#00172d" />
         </yAxis>
      </LineChart>
      <Label layoutX="31.0" layoutY="45.0" prefHeight="71.0" prefWidth="284.0" text="Reports">
