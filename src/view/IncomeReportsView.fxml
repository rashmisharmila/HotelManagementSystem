        <yAxis>
          <NumberAxis fx:id="income" label="Income(Rs)" side="LEFT" tickLabelFill="#00172d" />
        </yAxis>
      </LineChart>
      <LineChart fx:id="annuallinechart" layoutX="852.0" layoutY="184.0" prefHeight="815.0" prefWidth="777.0" title="Annually Income Report">
